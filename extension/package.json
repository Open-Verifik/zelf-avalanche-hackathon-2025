{
	"name": "zelf-keys-extension",
	"version": "1.0.0",
	"description": "Zelf Keys - Secure Digital Identity and Key Management Extension",
	"author": "Zelf Keys Team",
	"license": ".",
	"private": true,
	"scripts": {
		"build": "npm run build:prod",
		"build:ext:chrome": "ng build --configuration 'ext_prod_chrome'",
		"build:ext:firefox": "ng build --configuration 'ext_prod_firefox'",
		"build:ext": "npm run build:ext:chrome && npm run build:ext:firefox",
		"build:prod": "npm test && npm run bump:patch && npm run build:ext:chrome && npm run transloco:optimize",
		"build:test": "npm run test:run",
		"build:web:prod": "ng build --configuration production",
		"build:web": "npm run build:web:prod",
		"bump:major": "npm version major --no-git-tag-version && node scripts/sync-versions.js",
		"bump:minor": "npm version minor --no-git-tag-version && node scripts/sync-versions.js",
		"bump:patch": "npm version patch --no-git-tag-version && node scripts/sync-versions.js",
		"e2e": "ng e2e",
		"e2e:headless": "export E2E_HEADLESS=true && ng e2e",
		"start": "ng serve",
		"test": "npm run test:run",
		"test:ci": "ng test --browsers ChromeHeadlessCI --watch=false",
		"test:coverage": "ng test --code-coverage",
		"test:run": "ng test --browsers ChromeHeadless --watch=false",
		"i18n:find": "transloco-keys-manager find",
		"i18n:sync": "node scripts/sync-translations.js",
		"transloco:optimize": "transloco-optimize dist/prod/ext/chrome/assets/i18n",
		"watch": "ng build --watch --configuration development",
		"watch:ext": "ng build --watch --configuration 'ext_dev_chrome'",
		"watch:ext:firefox": "ng build --watch --configuration 'ext_dev_firefox' & web-ext run --source-dir './dist/dev/ext/firefox' --devtools",
		"puppeteer": "ng run zelf-extension:puppeteer"
	},
	"dependencies": {
		"@angular/animations": "19.2.11",
		"@angular/cdk": "^19.2.16",
		"@angular/common": "19.2.11",
		"@angular/compiler": "19.2.11",
		"@angular/core": "19.2.11",
		"@angular/flex-layout": "^15.0.0-beta.42",
		"@angular/forms": "19.2.11",
		"@angular/material": "19.2.16",
		"@angular/material-luxon-adapter": "19.2.16",
		"@angular/platform-browser": "19.2.11",
		"@angular/platform-browser-dynamic": "19.2.11",
		"@angular/router": "19.2.11",
		"@babel/runtime": "^7.26.0",
		"@bitcoinerlab/secp256k1": "^1.2.0",
		"@ethereumjs/wallet": "^2.0.3",
		"@jsverse/transloco": "^7.6.1",
		"@mysten/sui.js": "^0.54.1",
		"@openpgp/web-stream-tools": "^0.1.1",
		"@quicknode/sdk": "^2.4.2",
		"@scure/bip32": "^1.6.2",
		"@scure/bip39": "^1.5.4",
		"@solana/spl-token": "^0.4.13",
		"@tensorflow/tfjs-core": "^4.12.0",
		"@vladmandic/face-api": "^1.7.12",
		"bip32": "^5.0.0-rc.0",
		"bip39": "^3.1.0",
		"bitcoinjs-lib": "^6.1.7",
		"ecpair": "^3.0.0",
		"ed25519-hd-key": "^1.3.0",
		"ethers": "^6.12.1",
		"jsqr": "^1.4.0",
		"lightweight-charts": "^5.0.5",
		"micro-key-producer": "^0.7.5",
		"moment": "^2.29.4",
		"ngx-webcam": "^0.4.1",
		"openpgp": "^5.11.1",
		"perfect-scrollbar": "1.5.5",
		"qr-code-styling": "^1.9.1",
		"qrcode": "^1.5.4",
		"rxjs": "7.8.1",
		"web3": "^4.8.0",
		"web3-validator": "^2.0.6",
		"webextension-polyfill": "^0.12.0",
		"zone.js": "^0.15.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^19.2.9",
		"@angular/cli": "^19.2.9",
		"@angular/compiler-cli": "19.2.11",
		"@jsverse/transloco-keys-manager": "^6.1.0",
		"@jsverse/transloco-optimize": "^7.0.1",
		"@puppeteer/ng-schematics": "^0.7.0",
		"@types/bs58": "^4.0.4",
		"@types/chroma-js": "2.4.0",
		"@types/chrome": "^0.0.266",
		"@types/jasmine": "5.1.0",
		"@types/jasminewd2": "^2.0.10",
		"@types/lodash": "4.14.195",
		"@types/lodash-es": "4.17.7",
		"@types/luxon": "3.3.0",
		"@types/node-rsa": "^1.1.4",
		"@types/qrcode": "^1.5.5",
		"@types/webextension-polyfill": "^0.12.2",
		"assert": "^2.1.0",
		"autoprefixer": "10.4.14",
		"buffer": "^6.0.3",
		"chroma-js": "2.4.2",
		"crypto-browserify": "^3.12.1",
		"glob": "^11.0.2",
		"https-browserify": "^1.0.0",
		"jasmine": "5.7.0",
		"jasmine-core": "5.0.0",
		"karma": "^6.4.2",
		"karma-chrome-launcher": "^3.2.0",
		"karma-coverage": "^2.2.1",
		"karma-jasmine": "^5.1.0",
		"karma-jasmine-html-reporter": "^2.1.0",
		"lodash": "4.17.21",
		"ngx-build-plus": "^19.0.0",
		"os-browserify": "^0.3.0",
		"postcss": "^8.4.31",
		"prettier": "^3.5.3",
		"process": "^0.11.10",
		"puppeteer": "24.7.2",
		"stream-browserify": "^3.0.0",
		"stream-http": "^3.2.0",
		"ts-node": "^10.9.2",
		"typescript": "5.6",
		"url": "^0.11.4",
		"web-ext": "^8.6.0"
	}
}
